{
  "name": "usage-analytics-app",
  "version": "32.0.1",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "npm run lint:eslint && npm run lint:style",
    "lint:eslint": "eslint .",
    "lint:style": "d2-style js check --all",
    "eject": "react-scripts eject",
    "extract-pot": "d2-i18n-extract -p src/ -o i18n/",
    "localize": "yarn extract-pot && d2-i18n-generate -n usage-analytics-app -p ./i18n/ -o ./src/locales/",
    "manifest": "d2-manifest manifest.json ./public/manifest.webapp",
    "prestart": "yarn localize && yarn manifest",
    "prebuild": "yarn localize && yarn manifest",
    "pretest": "yarn localize && yarn manifest"
  },
  "dependencies": {
    "@dhis2/ui": "1.0.0-beta.14",
    "chart.js": "2.7.3",
    "core-js": "3.0.1",
    "lodash.debounce": "4.0.8",
    "react": "^16.8.6",
    "react-app-polyfill": "0.2.2",
    "react-chartjs-2": "2.7.4",
    "react-dom": "^16.8.6",
    "react-redux": "6.0.0",
    "react-scripts": "3.0.0",
    "redux": "4.0.1",
    "redux-thunk": "2.3.0",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "@dhis2/cli": "^1.0.2",
    "@dhis2/d2-i18n": "1.0.4",
    "@dhis2/d2-i18n-extract": "1.0.7",
    "@dhis2/d2-i18n-generate": "1.0.19",
    "d2-manifest": "^1.0.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "husky": "^2.2.0",
    "jest-enzyme": "^7.0.2",
    "prop-types": "^15.7.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "last 2 versions",
      "Firefox ESR",
      "ie 11",
      "not dead"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "d2-style commit check",
      "pre-commit": "d2-style js apply"
    }
  }
}
